{
  "name": "@lastfm-ts/api-types",
  "version": "0.0.4",
  "description": "Last.fm API Typings",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "format": "biome check --apply ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md", "package.json"],
  "keywords": ["lastfm", "lastfm-api", "api-types", "types", "typings"],
  "repository": "https://github.com/lastfm4devs/lastfm-ts",
  "author": "Davi Patricio <davipatricio@protonmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
