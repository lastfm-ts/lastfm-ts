{
  "name": "@lastfm-ts/wrapper",
  "version": "0.0.3",
  "description": "Easy-to-use Last.fm API Wrapper",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "format": "biome check --apply ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md", "package.json"],
  "keywords": ["lastfm", "lastfm-api", "api-wrapper"],
  "repository": "https://github.com/lastfm4devs/lastfm-ts",
  "author": "Davi Patricio <davipatricio@protonmail.com>",
  "license": "MIT",
  "dependencies": {
    "@lastfm-ts/api-types": "workspace:^"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "^20.13.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
